import React, { FunctionComponent } from 'react';
import {
    SVGIconPropsType,
    SVGIcon,
    SVGIconGroup,
    SVGIconPath,
} from '../../components/SVGIcon';

const <%- name %>IconShape = (
    <SVGIconGroup>
        <SVGIconPath 
            d="<%- shape %>"<% if(transform) { %>
            transform="<%- transform %>"<% } %>
        />
    </SVGIconGroup>
);

export const <%- name %>Icon: FunctionComponent<SVGIconPropsType> = props => {
    return <SVGIcon {...props}>{<%- name %>IconShape}</SVGIcon>;
};
