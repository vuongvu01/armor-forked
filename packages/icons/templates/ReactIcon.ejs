import React, { FunctionComponent } from 'react';
import {
    SVGIconPropsType,
    SVGIcon,
    SVGIconGroup,
    SVGIconPath,
} from '../../components/SVGIcon';

const <%- name %>IconShape = (
    <SVGIconGroup>
        <% paths.forEach(function(path) { %><SVGIconPath d="<%- path.d %>"<% if(path.transform) { %>transform="<%- path.transform %>"<% } %> /><% }); %>
    </SVGIconGroup>
);

export const <%- name %>Icon: FunctionComponent<SVGIconPropsType> = props => {
    const { className, ...restProps } = props;
    return (
        <SVGIcon
            {...restProps}
            className={`<%- name %>Icon ${className || ''}`}
        >
            {<%- name %>IconShape}
        </SVGIcon>
    );
};
